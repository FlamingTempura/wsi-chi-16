  urldate = {2014-09-04},
  journal = {{Canadian Journal of Anesthesia}},
  author = {Crosby, Edward},
  month = mar,
  year = {2007},
  keywords = {Anesthesiology,Cardiology,Intensive / Critical Care Medicine,Pain Medicine,Pediatrics,Pneumology/Respiratory System},
  pages = {227--241},
  file = {Full Text PDF:/home/peter/.zotero/zotero/7fs82xl7.default/zotero/storage/7B4WT384/Crosby - 2007 - Medical malpractice and anesthesiology literature.pdf:application/pdf;Snapshot:/home/peter/.zotero/zotero/7fs82xl7.default/zotero/storage/F2IZVQX2/BF03022645.html:text/html}
},



@inproceedings{Jacobs2014,
