  urldate = {2014-07-14},
  journal = {{Computer Methods and Programs in Biomedicine}},
  author = {Beuscart-Z\'{e}phir, M. C. and Brender, J. and Beuscart, R. and M\'{e}nager-Depriester, I.},
  month = sep,
  year = {1997},
  keywords = {Assessment,Cognitive evaluation,Medical informatics,information technology},
  pages = {19--28},
  file = {ScienceDirect Snapshot:/home/peter/.zotero/zotero/7fs82xl7.default/zotero/storage/4TJD5PZZ/S0169260797000308.html:text/html;ScienceDirect Full Text PDF:/home/peter/.zotero/zotero/7fs82xl7.default/zotero/storage/5ZIJ7ZB9/Beuscart-Zphir et al. - 1997 - Cognitive evaluation How to assess the usability .pdf:application/pdf}
