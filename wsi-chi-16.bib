	title = {Contribution of a Trunk Accelerometer System to the Characterization of Gait in Patients with Mild to Moderate Parkinson \#x2019;s disease.},
	volume = {{PP}},
	issn = {2168-2194},
	doi = {10.1109/JBHI.2015.2469540},
	abstract = {Objective: Gait disturbances like shuffling and short steps are obvious at visual observation in patients with advanced Parkinsons disease ({PD}). However, quantitative methods are increasingly used to evaluate the wide range of gait abnormalities that may occur over the disease course. The goal of this study was to test the ability of a trunk accelerometer system to quantify the effects of {PD} on several gait features when walking at self-selected speed. Methods: We recruited 96 subjects split into three age-matched groups: 32 healthy controls ({HC}), 32 {PD} patients at Hoehn and Yahr stage {\textless} {II} ({PD}-1), and 32 patients at Hoehn \& Yahr stage {II}-{III} ({PD}-2). The following outcomes were extracted from the signals of the tri-axial accelerometer worn on the lower back: stride length, cadence, regularity index, symmetry index and mechanical powers yielded in the cranial-caudal, antero-posterior and medial-lateral directions. Walking speed was measured using a stopwatch. Results: beside other gait features, the {PD}-1 and the {PD}-2 groups showed significantly reduced stride length normalized to height (p{\textless}0.02) and symmetry index (p{\textless}0.009) in comparison to the {HC}. Regularity index was the only feature significantly decreased in the {PD}-2 group as compared with the two other groups (p{\textless}0.01). The clinical relevance of this finding was supported by significant correlations with mobility and gait scales (r is around -0.3; p{\textless}0.05). Conclusion: Gait quantified by a trunk accelerometer may provide clinically useful information for the screening and follow-up of {PD} patients.},
	pages = {1--1},
	number = {99},
	journaltitle = {{IEEE} Journal of Biomedical and Health Informatics},
	author = {Demonceau, M. and Donneau, A. and Croisier, J. and Skawiniak, E. and Boutaayamou, M. and Maquet, D. and Garraux, G.},
	date = {2015},
  year = {2015},
	keywords = {Acceleration, accelerometer, Accelerometers, bradykinesia, Correlation, Diseases, Feature extraction, gait analysis, gait variability, Indexes, Legged locomotion, lit-review, Parkinsons disease},
	file = {IEEE Xplore Abstract Record:/home/peter/.zotero/zotero/hwyyl3u1.default/zotero/storage/JZ6A8NJE/abs_all.html:text/html;IEEE Xplore Full Text PDF:/home/peter/.zotero/zotero/hwyyl3u1.default/zotero/storage/KF82DSSX/Demonceau et al. - 2015 - Contribution of a Trunk Accelerometer System to th.pdf:application/pdf}
}

@article{dontje_measuring_2015,
	title = {Measuring steps with the Fitbit activity tracker: an inter-device reliability study},
	volume = {39},
	issn = {0309-1902},
	url = {http://www.tandfonline.com/doi/abs/10.3109/03091902.2015.1050125},
	doi = {10.3109/03091902.2015.1050125},
	shorttitle = {Measuring steps with the Fitbit activity tracker},
	abstract = {Activity trackers like Fitbit are used for self-tracking of physical activity by an increasing number of individuals. Comparing physical activity scores with peers can contribute to the desired behavioural change. However, for meaningful social comparison a high inter-device reliability is paramount. This study aimed to determine the inter-device reliability of Fitbit activity trackers in measuring steps. Ten activity trackers (Fitbit Ultra) were worn by a single person (male, 46 years) during eight consecutive days. Inter-device reliability was assessed on three different levels of aggregation (minutes, hours, days) with various methods, including intra-class correlation coefficient ({ICC}), Bland-Altman plots, limits of agreement ({LOA}) and Mixed Model Analysis. Results showed that the inter-device reliability of the Fitbit in measuring steps is good at all levels of aggregation (minutes, hours, days), but especially when steps were measured per day. This implies that individuals can reliably compare their daily physical activity scores with peers.},
	pages = {286--290},
	number = {5},
	journaltitle = {Journal of Medical Engineering \& Technology},
	author = {Dontje, Manon L. and Groot, Martijn de and Lengton, Remko R. and Schans, Cees P. van der and Krijnen, Wim P.},
	urldate = {2015-09-14},
