  author = {O'Loughlin, Gillian and Cullen, Sarah Jane and McGoldrick, Adrian and O'Connor, Siobhan and Blain, Richard and O'Malley, Shane and Warrington, Giles D.},
  month = mar,
  year = {2013},
  pages = {297--301},
  file = {ScienceDirect Snapshot:/home/peter/.zotero/zotero/7fs82xl7.default/zotero/storage/AJ2QERHS/S074937971200863X.html:text/html;ScienceDirect Full Text PDF:/home/peter/.zotero/zotero/7fs82xl7.default/zotero/storage/BGIBRAWN/O'Loughlin et al. - 2013 - Using a Wearable Camera to Increase the Accuracy o.pdf:application/pdf}
